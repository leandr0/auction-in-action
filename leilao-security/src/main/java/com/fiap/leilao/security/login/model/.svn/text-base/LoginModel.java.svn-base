package com.fiap.leilao.security.login.model;

import java.io.Serializable;

/**
 * @author leandro.goncalves
 *
 */
public class LoginModel implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 7971756038093982996L;

	private final String senha;
	
	private final String role;
	
	public LoginModel(String senha, String role) {
		this.senha = senha;
		this.role  = role;
	}

	/**
	 * @return the senha
	 */
	public String getSenha() {
		return senha;
	}

	/**
	 * @return the role
	 */
	public String getRole() {
		return role;
	}	
}
